<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ufersacc.bitniquel">
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        <activity android:name=".PrincipalActivity">
        </activity>
        <activity android:name=".RegistroActivity">
        </activity>
        <activity android:name=".RecebimentoActivity">
        </activity>
        <activity android:name=".EnvioActivity">
        </activity>
        <activity android:name=".PagamentoActivity">
        </activity>
        <activity android:name=".ExtratoActivity">
        </activity>
        <activity android:name=".TokenLoginActivity">
        </activity>
        <activity android:name=".AjustesActivity">
        </activity>
        <activity android:name=".GiftCardActivity">
        </activity>

    </application>

</manifest>